<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="scubs.push.server" class="Scubs\PushBundle\Server\Pusher">
        </service>
        <service id="scubs.push.message_dispatcher" class="Scubs\PushBundle\Server\MessageDispatcher">
            <argument>%scubs.push.pusher_name%</argument>
            <argument>%scubs.push.server_url%</argument>
        </service>
    </services>
</container>